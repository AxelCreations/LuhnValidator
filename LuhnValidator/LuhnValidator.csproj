<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>AxelCreations.Validators.LuhnValidator</PackageId>
    <Authors>AxelCreations</Authors>
    <Company>AxelCreations</Company>
    <Description>Credit Cards and Identification numbers Validator. Based on the Luhn Algorithm.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>Class Library</RepositoryType>
    <RepositoryUrl>https://github.com/AxelCreations/LuhnValidator</RepositoryUrl>
    <PackageTags>Validator, Luhn Algorithm, Luhn, CreditCard, Personal ID, AxelCreations</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>AxelCreations 2021</Copyright>
    <PackageReleaseNotes>Simple methods to perform the Credit Cards and Identification numbers validation</PackageReleaseNotes>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <Version>1.0.0.1</Version>
  </PropertyGroup>

</Project>
